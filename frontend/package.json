{
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:network": "vite --mode network --host",
    "serve:e2e:network": "cross-env VUE_DEVTOOLS_DISABLED=true npm run dev:network",
    "build": "vite build",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "init-tailwind": "tailwindcss init -p",
    "test:e2e:local": "cross-env ENV_FILE=.env.cy.local cypress run",
    "test:e2e:local:dev": "cross-env ENV_FILE=.env.cy.local cypress open",
    "test:e2e:network": "cross-env ENV_FILE=.env.cy.network cypress run",
    "test:e2e:network:dev": "cross-env ENV_FILE=.env.cy.network cypress open",
    "test:e2e:network:file": "cross-env ENV_FILE=.env.cy.network cypress run"
  },
  "dependencies": {
    "@heroicons/vue": "^2.2.0",
    "axios": "^1.8.4",
    "date-fns": "^4.1.0",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.1",
    "pinia": "^3.0.1",
    "tailwind-scrollbar-hide": "^4.0.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vue-toastification": "^2.0.0-rc.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.14.2",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.19",
    "cross-env": "^10.0.0",
    "cypress": "^15.0.0",
    "dotenv": "^17.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.26.0",
    "npm-run-all2": "^6.2.0",
    "postcss": "^8.4.38",
    "prettier": "^3.3.2",
    "tailwindcss": "^3.4.4",
    "typescript": "~5.4.0",
    "vite": "^5.3.1",
    "vite-plugin-vue-devtools": "^7.3.1",
    "vue-tsc": "^2.0.21"
  }
}
